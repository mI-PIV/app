<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="4dip" />
            <stroke android:width="1dip" android:color="#165A7D" />
            <gradient android:angle="-90" android:startColor="#165A7D" android:endColor="#165A7D"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="4dip" />
            <stroke android:width="1dip" android:color="#165A7D" />
            <solid android:color="#165A7D"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle"  >
            <corners android:radius="4dip" />
            <stroke android:width="1dip" android:color="#576674" />
            <solid android:color="#576674"/>
        </shape>
<!--        <color android:color="#576674">-->
<!--        </color>-->
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="4dip" />
            <stroke android:width="1dip" android:color="#165A7D" />
            <gradient android:angle="-90" android:startColor="#165A7D" android:endColor="#165A7D" />
        </shape>
    </item>
</selector>